{
  "name": "@countbot/djs-commander",
  "version": "2.0.0",
  "description": "Command handler for discord.js",
  "type": "module",
  "author": {
    "email": "contact.luyx.llc@gmail.com",
    "name": "Djs-Commander",
    "url": "https://luyx.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Count-Bot/djs-commander/issues",
    "email": "realcountbot@gmail.com"
  },
  "homepage": "https://github.com/Count-Bot/djs-commander#readme",
  "dependencies": {
    "@countbot/loggage": "^1.6.0",
    "discord-api-types": "^0.37.114",
    "ky": "^1.7.4",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.9.3",
    "eslint": "^9.16.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.3.2",
    "tsx": "^4.16.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.18.0"
  },
  "files": [
    "src",
    "build"
  ],
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:tests": "tsc --project tsconfig.test.json",
    "build:ci": "tsc --project tsconfig.build.json --noEmit",
    "format": "prettier --write . eslint --fix --format=pretty src",
    "lint": "prettier --check . eslint --format=pretty src",
    "prepare": "npm run build",
    "test:lcov": "node --import tsx --test --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=lcov.info --experimental-strip-types --test-coverage-exclude=**/*.js",
    "test:spec": "node --import tsx --test --experimental-test-coverage --test-reporter=spec --test-reporter-destination=stdout --experimental-strip-types --test-coverage-exclude=**/*.js",
    "test:ci": "node --import tsx --test",
    "test:local": "npm run build:tests && npm run test:spec && npm run test:lcov"
  }
}

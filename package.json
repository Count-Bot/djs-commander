  "description": "Command handler for discord.js",
  "type": "module",
  "author": {
    "email": "contact.luyx.llc@gmail.com",
    "name": "Djs-Commander",
    "url": "https://luyx.dev"
  },
  "license": "MIT",
  "bugs": {
